			box-sizing: border-box;
			height: 100%
			width: 100%
			padding: 15px
			text-decoration: none
			background-color: #eee
			border: 1px solid #ccc
			transition(all 1s ease-in-out)

			&:hover
				background-color: #ddd
				border-color: #bbb
				transition(all .2s ease-in-out)

			&:active
				background-color: #ccc
				border-color: #999
				color: #777
				transition(all .1s ease-in-out)



